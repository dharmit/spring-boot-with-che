{
  "description": "Spring Boot Stack on CentOS.",
  "source": {
    "origin": "gytis/centos_spring_boot",
    "type": "image"
  },
  "scope": "advanced",
  "tags": [
    "Java",
    "JDK",
    "Maven",
    "Spring Boot",
    "CentOS",
    "Git"
  ],
  "creator": "Gytis Trikleris",
  "workspaceConfig": {
    "environments": {
      "default": {
        "recipe": {
          "location": "gytis/centos_spring_boot",
          "type": "dockerimage"
        },
        "machines": {
          "dev-machine": {
            "servers": {},
            "agents": [
              "org.eclipse.che.terminal",
              "org.eclipse.che.ws-agent",
              "org.eclipse.che.ssh"
            ],
            "attributes": {
              "memoryLimitBytes": "2147483648"
            }
          }
        }
      }
    },
    "defaultEnv": "default",
    "commands": [
      {
        "commandLine": "scl enable rh-maven33 'mvn clean install -f ${current.project.path}'",
        "name": "build",
        "type": "mvn",
        "attributes": {}
      },
      {
        "commandLine": "cd ${current.project.path} && scl enable rh-maven33 'java -jar target/*.jar'",
        "name": "run",
        "type": "custom",
        "attributes": {
          "previewUrl": "http://${server.port.8080}"
        }
      }
    ],
    "projects": [],
    "name": "default",
    "links": []
  },
  "components": [
    {
      "version": "---",
      "name": "CentOS"
    },
    {
      "version": "1.8.0_45",
      "name": "JDK"
    },
    {
      "version": "3.2.2",
      "name": "Maven"
    }
  ],
  "name": "Spring Boot",
  "id": "spring-boot"
}